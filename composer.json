{
    "name": "palak-rajput/laravel-data-encryption",
    "description": "Encrypt sensitive data while maintaining searchability with Meilisearch",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Palak Rajput",
        "email": "palak@example.com"
    }],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "5.5.*|5.6.*|5.7.*|5.8.*|6.*|7.*|8.*|9.*|10.*|11.*",
        "ext-openssl": "*",
        "meilisearch/meilisearch-php": "^0.24.0|^0.25.0|^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "PalakRajput\\DataEncryption\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PalakRajput\\DataEncryption\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "PalakRajput\\DataEncryption\\Providers\\DataEncryptionServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}